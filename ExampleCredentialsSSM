INSERTAR CREDENCIALES EN AWS SSM PARAMETER STORE USANDO AWS CLI:
-----------------------------------------------------------------------------------------------------------------

aws ssm put-parameter --name "/cloud-cicd/docker-credentials/username" --value "ingresar_usuario_docker" --type "String"


aws ssm put-parameter --name "/cloud-cicd/docker-credentials/password" --value "ingresar_contrase√±a_docker" --type "SecureString"

aws ssm put-parameter --name "/cloud-cicd/docker-registry/url" --value "docker.io" --type "String"
